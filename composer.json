{
  "name": "plaisio/console",
  "description": "PhpPlaisio: Console",
  "keywords": [
    "PhpPlaisio",
    "Console"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-inotify": "*",
    "ext-libxml": "*",
    "plaisio/kernel": "^3.2.0",
    "setbased/error-handler": "^1.3.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-program-execution": "^1.2.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-sqlite-pdo": "^1.7.0",
    "setbased/typed-config": "^2.0.0",
    "symfony/console": "^6.2.5",
    "symfony/filesystem": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/plaisio"
  ],
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
