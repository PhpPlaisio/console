  "keywords": [
    "PhpPlaisio",
    "Console"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "symfony/console": "^4.0.0",
    "composer/composer": "^1.0.0"
  },
  "require-dev": {
