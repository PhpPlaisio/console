{
  "name": "plaisio/console",
  "description": "PhpPlaisio: Console",
  "keywords": [
    "PhpPlaisio",
    "Console"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-dom": "*",
    "ext-inotify": "*",
    "ext-libxml": "*",
    "plaisio/kernel": "^3.1.1",
    "setbased/error-handler": "^1.2.0",
    "setbased/exception": "^2.2.0",
    "setbased/helper-program-execution": "^1.1.0",
    "setbased/php-stratum": "^6.1.0",
    "setbased/php-stratum-sqlite-pdo": "^1.6.0",
    "setbased/typed-config": "^1.1.0",
    "symfony/console": "^5.3.10",
    "webmozart/path-util": "^2.3.0"
  },
  "require-dev": {
    "phing/phing": "^2.17.0",
    "setbased/phing-extensions": "^2.1.0",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/plaisio"
  ],
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
