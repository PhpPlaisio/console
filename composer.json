{
  "name": "plaisio/console",
  "description": "PhpPlaisio: Console",
  "keywords": [
    "PhpPlaisio",
    "Console"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/kernel": "^3.2.1",
    "setbased/error-handler": "^1.3.0",
    "setbased/exception": "^2.4.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/filesystem": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/plaisio"
  ],
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
